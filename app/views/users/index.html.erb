<h1>All Users</h1>

<table class="table">
  <thead>
    <th>Email</th>
    <th>&nbsp;</th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.email, user %></td>
        <td>
          <% if !current_user?(user) %>
            <%= link_to "delete", user, 
                                  method: :delete, 
                                  data: { confirm: "Are you sure?" } %></td>
          <% else %>
            &nbsp;
          <% end %>
      </tr>
    <% end %>
  </tbody>
</table>